    "es"
  ],
  "main": "es/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "sh scripts/build-components.sh",
    "buildDev": "babel ./src --ignore '**/*.stories.jsx' --copy-files --delete-dir-on-start --out-dir es --watch",
    "lint": "eslint --ext js,jsx src test",
    "test:jest": "jest test --coverage",
