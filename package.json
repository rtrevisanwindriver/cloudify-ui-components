    "storybook": "start-storybook --docs -p 9001",
    "build-storybook": "build-storybook --docs -c .storybook",
    "publish:major": "sh scripts/publish-version.sh major",
    "publish:minor": "sh scripts/publish-version.sh minor",
    "publish:patch": "sh scripts/publish-version.sh patch"
  },
  "keywords": [
    "react",
    "shared",
    "components",
