{
  "name": "cloudify-ui-components",
  "version": "2.16.0",
  "description": "Cloudify UI React components library",
  "homepage": "https://github.com/cloudify-cosmo/cloudify-ui-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudify-cosmo/cloudify-ui-components.git"
  },
  "license": "Apache-2.0",
  "author": "Cloudify Platform Ltd. <cosmo-admin@cloudify.co>",
  "files": [
    "es",
    "src",
    "typings"
  ],
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "typings/components/index.d.ts",
  "scripts": {
    "audit": "npm audit --production",
    "build": "bash scripts/build-components.sh && npm run generate-type-declarations",
    "buildDev": "babel ./src/components --ignore '**/*.stories.{jsx,tsx}' --copy-files --delete-dir-on-start --extensions '.ts,.tsx,.js,.jsx' --out-dir es --watch",
    "check-types": "tsc -b",
    "generate-type-declarations": "tsc -b src",
    "lint": "eslint --cache --ignore-path .gitignore --ext js,jsx,ts,tsx .",
    "test:jest": "jest test --coverage --maxWorkers=2",
    "test:loki": "loki --diffingEngine gm --chromeTolerance 0.9 --requireReference --reactUri file:./storybook-static",
    "test": "npm run lint && npm run test:jest",
    "storybook": "start-storybook --docs -p 9001",
    "build-storybook": "build-storybook --docs -c .storybook --quiet",
    "publish:major": "bash scripts/publish-version.sh major",
    "publish:minor": "bash scripts/publish-version.sh minor",
    "publish:patch": "bash scripts/publish-version.sh patch"
  },
  "keywords": [
    "react",
    "shared",
    "components",
    "cloudify",
    "ui"
  ],
  "bugs": {
    "url": "https://cloudify.co/slack/"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-docs": "^6.4.18",
    "@storybook/addon-storyshots": "^6.4.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.18",
    "@storybook/theming": "^6.4.18",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.20.43",
    "@types/prop-types": "^15.7.4",
    "@types/react-color": "^3.0.6",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.5.0",
    "lodash": "^4.17.21",
    "loki": "^0.28.1",
    "moment": "^2.29.1",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "sass": "^1.49.7",
    "sass-loader": "^7.3.1",
    "semantic-ui-css-offline": "^1.0.4-2.4.2",
    "semantic-ui-react": "^2.1.1",
    "style-loader": "^1.3.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cloudify-ui-common": "^7.2.0",
    "rc-tree": "^2.1.4",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.0.4",
    "react-datepicker": "1.8.0",
    "react-json-view": "^1.21.3",
    "react-live": "^2.4.1",
    "react-syntax-highlighter": "^15.4.5",
    "react-transition-group": "^4.4.2",
    "tinycolor2": "^1.4.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.6.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "semantic-ui-css-offline": "^1.0.4-2.4.2",
    "semantic-ui-react": "^2.0.1",
    "styled-components": "^5.2.1"
  },
  "loki": {
    "verboseRenderer": true,
    "chromeSelector": "body > div#root > div > *",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  }
}
